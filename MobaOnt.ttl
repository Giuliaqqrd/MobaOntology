@prefix vann: <http://purl.org/vocab/vann/> .
@prefix event: <http://purl.org/NET/c4dm/event.owl#> .
@prefix sport: <https://www.bbc.co.uk/ontologies/sport/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix schema: <https://schema.org/> .
@prefix dbpedia-owl: <http://dbpedia.org/ontology/> .
@base <http://www.semanticweb.org/MobaOntology#> .

<http://www.semanticweb.org/MobaOntology> rdf:type owl:Ontology ;
                                           dc:creator "Giulia Coucourde"@en ,
                                                      "Laurentiu Jr Marius Zaharia"@en ;
                                           dc:date "2024-06-18"@en ;
                                           dc:description "An ontology for representing concepts and entities in Multiplayer Online Battle Arena (MOBA) games."@en ;
                                           dc:title "MOBA Game Ontology"@en ;
                                           vann:preferredNamespacePrefix "moba" ;
                                           vann:preferredNamespaceUri "http://www.semanticweb.org/MobaOntology#" ;
                                           rdfs:label "MOBA Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/NET/c4dm/event.owl#term_status
event:term_status rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/innovation/ns#hasDeveloped
inno:hasDeveloped rdf:type owl:ObjectProperty ;
                  owl:inverseOf :hasDeveloper ;
                  rdfs:domain :Developer ;
                  rdfs:range vgo:Game ;
                  rdfs:comment "Relates innovator to the innovations they have developed. Innovators can be groups, organisations or people."@en ;
                  rdfs:label "hasDeveloped"@en .


###  http://purl.org/net/videogameontology#hasPlayingArea
vgo:hasPlayingArea rdf:type owl:ObjectProperty ;
                   owl:equivalentProperty :hasMapArea ;
                   rdfs:label "vgo:hasPlayingArea"@en .


###  http://purl.org/net/videogameontology#isItemInGame
vgo:isItemInGame rdf:type owl:ObjectProperty ;
                 rdfs:domain vgo:Item ;
                 rdfs:range vgo:Game ;
                 rdfs:comment "The vgo:isItemInGame is used to specify which item belongs to a particular game. An item cannot exist alone and thus should always be associated to a certain game."@en ;
                 rdfs:label "isItemInGame"@en .


###  http://purl.org/net/videogameontology#isSessionInGame
vgo:isSessionInGame rdf:type owl:ObjectProperty ;
                    owl:equivalentProperty :isMatchInGame ;
                    rdfs:comment "The vgo:isSessionInGame property links a vgo:Session to a certain vgo:Game. Each gameplay session must belong to a certain game."@en ;
                    rdfs:label "vgo:isSessionInGame"@en .


###  http://purl.org/net/videogameontology#ownsCharacter
vgo:ownsCharacter rdf:type owl:ObjectProperty ;
                  owl:equivalentProperty :ownsChampion ;
                  rdfs:comment "The vgo:ownsCharacter property asserts which characters are owned by a specific player. A player can have multiple characters in one game and this connection is used to define all the different characters a player could be playing. Even though a player deletes, trades or loses his/her character in any way, the connection can be kept to contain the player's history of owned characters."@en ;
                  rdfs:label "vgo:ownsCharacter"@en .


###  http://purl.org/net/videogameontology#ownsItem
vgo:ownsItem rdf:type owl:ObjectProperty ;
             owl:equivalentProperty :suitableItem ;
             rdfs:label "vgo:ownsItem"@en .


###  http://purl.org/net/videogameontology#purchasesGameOffering
vgo:purchasesGameOffering rdf:type owl:ObjectProperty ;
                          rdfs:domain vgo:InAppPurchaseEvent ;
                          rdfs:range vgo:GameProduct .


###  http://www.semanticweb.org/MobaOntology#availableOnPlatform
:availableOnPlatform rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isPlatformOf ;
                     rdfs:domain vgo:Game ;
                     rdfs:range :Platform ;
                     rdfs:comment "Specifies the gaming platform(s) on which a MOBA game is available. This property links a game to one or more platforms."@en ;
                     rdfs:label "availableOnPlatform"@en .


###  http://www.semanticweb.org/MobaOntology#containsElement
:containsElement rdf:type owl:ObjectProperty ;
                 owl:propertyChainAxiom ( :hasMapArea
                                          :hasMapElement
                                        ) .


###  http://www.semanticweb.org/MobaOntology#follows
:follows rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#hasAbility
:hasAbility rdf:type owl:ObjectProperty ;
            owl:inverseOf :isAbilityOf ;
            rdfs:domain :Champion ;
            rdfs:range :Ability .


###  http://www.semanticweb.org/MobaOntology#hasBuild
:hasBuild rdf:type owl:ObjectProperty ;
          owl:inverseOf :isBuildOf ;
          rdfs:domain :Champion ;
          rdfs:range :Build .


###  http://www.semanticweb.org/MobaOntology#hasCompetitor
:hasCompetitor rdf:type owl:ObjectProperty ;
               owl:inverseOf :isCompetitorOf .


###  http://www.semanticweb.org/MobaOntology#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isComponentOf ;
              rdfs:domain :CompositeItem ;
              rdfs:range vgo:Item .


###  http://www.semanticweb.org/MobaOntology#hasDeveloper
:hasDeveloper rdf:type owl:ObjectProperty ;
              rdfs:domain vgo:Game ;
              rdfs:range :Developer ;
              rdfs:comment "Specifies the developer or development team responsible for creating a game in the context of MOBA games. Is the inverse property of hasDeveloped."@en ;
              rdfs:label "hasDeveloper"@en .


###  http://www.semanticweb.org/MobaOntology#hasDiscipline
:hasDiscipline rdf:type owl:ObjectProperty ;
               owl:inverseOf :isDisciplineOf ;
               rdfs:domain sport:Competition ;
               rdfs:range vgo:Game .


###  http://www.semanticweb.org/MobaOntology#hasItemSet
:hasItemSet rdf:type owl:ObjectProperty ;
            owl:inverseOf :isItemSetOf ;
            rdfs:domain :Build ;
            rdfs:range vgo:Item .


###  http://www.semanticweb.org/MobaOntology#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             owl:inverseOf :isLocationOf ;
             rdfs:domain sport:Competition ;
             rdfs:range :Location .


###  http://www.semanticweb.org/MobaOntology#hasMap
:hasMap rdf:type owl:ObjectProperty ;
        rdfs:domain vgo:Game ;
        rdfs:range :Map .


###  http://www.semanticweb.org/MobaOntology#hasMapArea
:hasMapArea rdf:type owl:ObjectProperty ;
            rdfs:domain :Map ;
            rdfs:range :MapArea .


###  http://www.semanticweb.org/MobaOntology#hasMapElement
:hasMapElement rdf:type owl:ObjectProperty ;
               owl:inverseOf :isMapElementOf ;
               rdfs:domain :MapArea ;
               rdfs:range :MapElement .


###  http://www.semanticweb.org/MobaOntology#hasMatch
:hasMatch rdf:type owl:ObjectProperty ;
          owl:inverseOf :isMatchInGame ;
          rdfs:domain vgo:Game ;
          rdfs:range :Match .


###  http://www.semanticweb.org/MobaOntology#hasMechanic
:hasMechanic rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain :Team ;
           rdfs:range vgo:Player .


###  http://www.semanticweb.org/MobaOntology#hasMonetization
:hasMonetization rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isMonetizationOf ;
                 rdfs:domain vgo:Game ;
                 rdfs:range :MonetizationModel ;
                 rdfs:comment "Specifies the monetization model used by a MOBA game. This property links a game to its monetization model, such as PayToPlay or FreeToPlay."@en ;
                 rdfs:label "hasMonetization"@en .


###  http://www.semanticweb.org/MobaOntology#hasOutcome
:hasOutcome rdf:type owl:ObjectProperty ;
            owl:inverseOf :isOutcomeOf ;
            rdfs:range :GameOutcome .


###  http://www.semanticweb.org/MobaOntology#hasRank
:hasRank rdf:type owl:ObjectProperty ;
         rdfs:range :Rank .


###  http://www.semanticweb.org/MobaOntology#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         owl:inverseOf :isRoleOf ;
         rdfs:domain :Champion ;
         rdfs:range :Role .


###  http://www.semanticweb.org/MobaOntology#includesTeam
:includesTeam rdf:type owl:ObjectProperty ;
              rdfs:domain :Match ;
              rdfs:range :Team .


###  http://www.semanticweb.org/MobaOntology#isAbilityOf
:isAbilityOf rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Ability ;
             rdfs:range :Champion .


###  http://www.semanticweb.org/MobaOntology#isBuildOf
:isBuildOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Build ;
           rdfs:range :Champion .


###  http://www.semanticweb.org/MobaOntology#isCompetitorOf
:isCompetitorOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#isComponentOf
:isComponentOf rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain vgo:Item ;
               rdfs:range :CompositeItem .


###  http://www.semanticweb.org/MobaOntology#isDisciplineOf
:isDisciplineOf rdf:type owl:ObjectProperty ;
                rdfs:domain vgo:Game ;
                rdfs:range sport:Competition .


###  http://www.semanticweb.org/MobaOntology#isHighestElo
:isHighestElo rdf:type owl:ObjectProperty ;
              rdfs:domain :Elite ;
              rdfs:range :Rank .


###  http://www.semanticweb.org/MobaOntology#isItemSetOf
:isItemSetOf rdf:type owl:ObjectProperty ;
             rdfs:domain vgo:Item ;
             rdfs:range :Build .


###  http://www.semanticweb.org/MobaOntology#isLocationOf
:isLocationOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Location ;
              rdfs:range sport:Competition .


###  http://www.semanticweb.org/MobaOntology#isMapElementOf
:isMapElementOf rdf:type owl:ObjectProperty ;
                rdfs:domain :MapElement ;
                rdfs:range :MapArea .


###  http://www.semanticweb.org/MobaOntology#isMatchInGame
:isMatchInGame rdf:type owl:ObjectProperty ;
               rdfs:domain :Match ;
               rdfs:range vgo:Game .


###  http://www.semanticweb.org/MobaOntology#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain vgo:Player ;
            rdfs:range :Team .


###  http://www.semanticweb.org/MobaOntology#isMonetizationOf
:isMonetizationOf rdf:type owl:ObjectProperty ;
                  rdfs:domain :MonetizationModel ;
                  rdfs:range vgo:Game .


###  http://www.semanticweb.org/MobaOntology#isNonPlayableOf
:isNonPlayableOf rdf:type owl:ObjectProperty ;
                 rdfs:domain vgo:Character ;
                 rdfs:range vgo:Game ;
                 rdfs:comment "Indicates a non-playable entity within a game. This property links a non-playable entity to the game in which it appears but cannot be played."@en ;
                 rdfs:label "isNonPlayableOf"@en .


###  http://www.semanticweb.org/MobaOntology#isOutcomeOf
:isOutcomeOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#isOwnedByPlayer
:isOwnedByPlayer rdf:type owl:ObjectProperty ;
                 owl:inverseOf :ownsChampion ;
                 rdfs:domain :Champion ;
                 rdfs:range vgo:Player ;
                 rdfs:comment "Indicates that a champion in MOBA games is owned or possessed by a player. This property represents the reverse relationship of ownsChampion."@en ;
                 rdfs:label "isOwnedByPlayer"@en .


###  http://www.semanticweb.org/MobaOntology#isPlatformOf
:isPlatformOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Platform ;
              rdfs:range vgo:Game .


###  http://www.semanticweb.org/MobaOntology#isPlayableOf
:isPlayableOf rdf:type owl:ObjectProperty ;
              rdfs:domain vgo:Character ;
              rdfs:range vgo:Game ;
              rdfs:comment "Indicates an entity that is a playable within a game. This property links a playable character to the game in which it can be played."@en ;
              rdfs:label "isPlayableOf"@en .


###  http://www.semanticweb.org/MobaOntology#isPlayedByTeam
:isPlayedByTeam rdf:type owl:ObjectProperty ;
                owl:propertyChainAxiom ( :hasMatch
                                         :includesTeam
                                       ) .


###  http://www.semanticweb.org/MobaOntology#isRoleOf
:isRoleOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Role ;
          rdfs:range :Champion .


###  http://www.semanticweb.org/MobaOntology#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Creature ;
           rdfs:range :MapArea .


###  http://www.semanticweb.org/MobaOntology#ownsChampion
:ownsChampion rdf:type owl:ObjectProperty ;
              rdfs:domain vgo:Player ;
              rdfs:range :Champion ;
              rdfs:comment "Indicates ownership or possession of a playable champion by a player in MOBA games. A champion referred to by this property is actively played by the owner, distinguishing it from non-playable characters."@en ;
              rdfs:label "ownsChampion"@en .


###  http://www.semanticweb.org/MobaOntology#precedes
:precedes rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty .


###  http://www.semanticweb.org/MobaOntology#suitableItem
:suitableItem rdf:type owl:ObjectProperty ;
              owl:propertyChainAxiom ( :hasBuild
                                       :hasItemSet
                                     ) .


