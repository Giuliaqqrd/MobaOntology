                                                          owl:someValuesFrom :Rank
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Match .


###  http://www.semanticweb.org/MobaOntology#RegionalChampionship
:RegionalChampionship rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                   owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :hasLocation ;
                                                                                   owl:hasValue :America
                                                                                 ]
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :hasLocation ;
                                                                                   owl:hasValue :Asia
                                                                                 ]
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :hasLocation ;
                                                                                   owl:hasValue :Europa
                                                                                 ]
                                                                               )
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasCompetitor ;
                                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :Competitors
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :isOfficialCompetition ;
                                                                   owl:hasValue "true"^^xsd:boolean
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf :Championship ;
                      rdfs:comment "Regional is a subclass of Championship in the context of MOBAs, referring to tournaments that focus on specific geographic regions. Regional championships feature teams or players from a particular region competing to qualify for higher-level competitions or to claim regional titles."@en ;
                      rdfs:label "RegionalChampionship"@en .


###  http://www.semanticweb.org/MobaOntology#Resources
