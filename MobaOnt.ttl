@prefix : <http://www.semanticweb.org/MobaOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix vgo: <http://purl.org/net/videogameontology#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix inno: <http://purl.org/innovation/ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix event: <http://purl.org/NET/c4dm/event.owl#> .
@prefix sport: <https://www.bbc.co.uk/ontologies/sport/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix schema: <https://schema.org/> .
@prefix dbpedia-owl: <http://dbpedia.org/ontology/> .
@base <http://www.semanticweb.org/MobaOntology#> .

<http://www.semanticweb.org/MobaOntology> rdf:type owl:Ontology ;
                                           dc:creator "Giulia Coucourde"@en ,
                                                      "Laurentiu Jr Marius Zaharia"@en ;
                                           dc:date "2024-06-18"@en ;
                                           dc:description "An ontology for representing concepts and entities in Multiplayer Online Battle Arena (MOBA) games."@en ;
                                           dc:title "MOBA Game Ontology"@en ;
                                           vann:preferredNamespacePrefix "moba" ;
                                           vann:preferredNamespaceUri "http://www.semanticweb.org/MobaOntology#" ;
                                           rdfs:label "MOBA Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/NET/c4dm/event.owl#term_status
event:term_status rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/innovation/ns#hasDeveloped
inno:hasDeveloped rdf:type owl:ObjectProperty ;
                  owl:inverseOf :hasDeveloper ;
                  rdfs:domain :Developer ;
                  rdfs:range vgo:Game ;
                  rdfs:comment "Relates innovator to the innovations they have developed. Innovators can be groups, organisations or people."@en ;
                  rdfs:label "hasDeveloped"@en .


###  http://purl.org/net/videogameontology#hasPlayingArea
vgo:hasPlayingArea rdf:type owl:ObjectProperty ;
                   owl:equivalentProperty :hasMapArea ;
                   rdfs:label "vgo:hasPlayingArea"@en .


###  http://purl.org/net/videogameontology#isItemInGame
vgo:isItemInGame rdf:type owl:ObjectProperty ;
                 rdfs:domain vgo:Item ;
                 rdfs:range vgo:Game ;
                 rdfs:comment "The vgo:isItemInGame is used to specify which item belongs to a particular game. An item cannot exist alone and thus should always be associated to a certain game."@en ;
                 rdfs:label "isItemInGame"@en .


###  http://purl.org/net/videogameontology#isSessionInGame
vgo:isSessionInGame rdf:type owl:ObjectProperty ;
                    owl:equivalentProperty :isMatchInGame ;
                    rdfs:comment "The vgo:isSessionInGame property links a vgo:Session to a certain vgo:Game. Each gameplay session must belong to a certain game."@en ;
                    rdfs:label "vgo:isSessionInGame"@en .


###  http://purl.org/net/videogameontology#ownsCharacter
vgo:ownsCharacter rdf:type owl:ObjectProperty ;
                  owl:equivalentProperty :ownsChampion ;
                  rdfs:comment "The vgo:ownsCharacter property asserts which characters are owned by a specific player. A player can have multiple characters in one game and this connection is used to define all the different characters a player could be playing. Even though a player deletes, trades or loses his/her character in any way, the connection can be kept to contain the player's history of owned characters."@en ;
                  rdfs:label "vgo:ownsCharacter"@en .


###  http://purl.org/net/videogameontology#ownsItem
vgo:ownsItem rdf:type owl:ObjectProperty ;
             owl:equivalentProperty :suitableItem ;
             rdfs:label "vgo:ownsItem"@en .


###  http://purl.org/net/videogameontology#purchasesGameOffering
vgo:purchasesGameOffering rdf:type owl:ObjectProperty ;
                          rdfs:domain vgo:InAppPurchaseEvent ;
                          rdfs:range vgo:GameProduct .


###  http://www.semanticweb.org/MobaOntology#availableOnPlatform
:availableOnPlatform rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isPlatformOf ;
                     rdfs:domain vgo:Game ;
                     rdfs:range :Platform ;
                     rdfs:comment "Specifies the gaming platform(s) on which a MOBA game is available. This property links a game to one or more platforms."@en ;
                     rdfs:label "availableOnPlatform"@en .


###  http://www.semanticweb.org/MobaOntology#containBase
:containBase rdf:type owl:ObjectProperty ;
             owl:propertyChainAxiom ( :hasMapSide
                                      :hasBase
                                    ) .


###  http://www.semanticweb.org/MobaOntology#hasAbilitySet
:hasAbilitySet rdf:type owl:ObjectProperty ;
               owl:inverseOf :isAbilitySetOf ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Champion ;
               rdfs:range :Abilities .


###  http://www.semanticweb.org/MobaOntology#hasBase
:hasBase rdf:type owl:ObjectProperty ;
         owl:inverseOf :isBaseOf ;
         rdfs:domain :MapSide ;
         rdfs:range :Base .


###  http://www.semanticweb.org/MobaOntology#hasBuild
:hasBuild rdf:type owl:ObjectProperty ;
          rdfs:domain :Champion ;
          rdfs:range :Build .


###  http://www.semanticweb.org/MobaOntology#hasCompetitor
:hasCompetitor rdf:type owl:ObjectProperty ;
               owl:inverseOf :isCompetitorOf .


###  http://www.semanticweb.org/MobaOntology#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isComponentOf ;
              rdfs:domain :CompositeItem ;
              rdfs:range vgo:Item .


###  http://www.semanticweb.org/MobaOntology#hasDeveloper
:hasDeveloper rdf:type owl:ObjectProperty ;
              rdfs:domain vgo:Game ;
              rdfs:range :Developer ;
              rdfs:comment "Specifies the developer or development team responsible for creating a game in the context of MOBA games. Is the inverse property of hasDeveloped."@en ;
              rdfs:label "hasDeveloper"@en .


###  http://www.semanticweb.org/MobaOntology#hasItemSet
:hasItemSet rdf:type owl:ObjectProperty ;
            owl:inverseOf :isItemSetOf ;
            rdfs:domain :Build ;
            rdfs:range vgo:Item .


###  http://www.semanticweb.org/MobaOntology#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             owl:inverseOf :isLocationOf .


###  http://www.semanticweb.org/MobaOntology#hasMapArea
:hasMapArea rdf:type owl:ObjectProperty ;
            rdfs:domain :Map ;
            rdfs:range :MapArea .


###  http://www.semanticweb.org/MobaOntology#hasMapElement
:hasMapElement rdf:type owl:ObjectProperty ;
               rdfs:domain :MapArea ;
               rdfs:range :MapElement .


###  http://www.semanticweb.org/MobaOntology#hasMapSide
:hasMapSide rdf:type owl:ObjectProperty ;
            rdfs:domain :Map ;
            rdfs:range :MapSide .


###  http://www.semanticweb.org/MobaOntology#hasMatch
:hasMatch rdf:type owl:ObjectProperty ;
          owl:inverseOf :isMatchInGame ;
          rdfs:domain vgo:Game ;
          rdfs:range :Match .


###  http://www.semanticweb.org/MobaOntology#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain :Team ;
           rdfs:range vgo:Player .


###  http://www.semanticweb.org/MobaOntology#hasMonetization
:hasMonetization rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isMonetizationOf ;
                 rdfs:domain vgo:Game ;
                 rdfs:comment "Specifies the monetization model used by a MOBA game. This property links a game to its monetization model, such as PayToPlay or FreeToPlay."@en ;
                 rdfs:label "hasMonetization"@en .


###  http://www.semanticweb.org/MobaOntology#hasOfficialTournaments
:hasOfficialTournaments rdf:type owl:ObjectProperty ;
                        rdfs:domain vgo:Game ;
                        rdfs:range :OfficialCompetition .


###  http://www.semanticweb.org/MobaOntology#hasOutcome
:hasOutcome rdf:type owl:ObjectProperty ;
            owl:inverseOf :isOutcomeOf ;
            rdfs:range :GameOutcome .


###  http://www.semanticweb.org/MobaOntology#hasRank
:hasRank rdf:type owl:ObjectProperty ;
         rdfs:range :Rank .


###  http://www.semanticweb.org/MobaOntology#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         owl:inverseOf :isRoleOf ;
         rdfs:domain :Champion ;
         rdfs:range :Role .


###  http://www.semanticweb.org/MobaOntology#includes
:includes rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty .


###  http://www.semanticweb.org/MobaOntology#includesTeam
:includesTeam rdf:type owl:ObjectProperty ;
              rdfs:domain :Match ;
              rdfs:range :Team .


###  http://www.semanticweb.org/MobaOntology#isAbilitySetOf
:isAbilitySetOf rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Abilities ;
                rdfs:range :Champion .


###  http://www.semanticweb.org/MobaOntology#isBaseOf
:isBaseOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Base ;
          rdfs:range :MapSide .


###  http://www.semanticweb.org/MobaOntology#isCompetitorOf
:isCompetitorOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#isComponentOf
:isComponentOf rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain vgo:Item ;
               rdfs:range :CompositeItem .


###  http://www.semanticweb.org/MobaOntology#isItemSetOf
:isItemSetOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain vgo:Item ;
             rdfs:range :Build .


###  http://www.semanticweb.org/MobaOntology#isLocationOf
:isLocationOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#isMatchInGame
:isMatchInGame rdf:type owl:ObjectProperty ;
               rdfs:domain :Match ;
               rdfs:range vgo:Game .


###  http://www.semanticweb.org/MobaOntology#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain vgo:Player ;
            rdfs:range :Team .


###  http://www.semanticweb.org/MobaOntology#isMonetizationOf
:isMonetizationOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#isNonPlayableOf
:isNonPlayableOf rdf:type owl:ObjectProperty ;
                 rdfs:comment "Indicates a non-playable entity within a game. This property links a non-playable entity to the game in which it appears but cannot be played."@en ;
                 rdfs:label "isNonPlayableOf"@en .


###  http://www.semanticweb.org/MobaOntology#isOutcomeOf
:isOutcomeOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/MobaOntology#isOwnedByPlayer
:isOwnedByPlayer rdf:type owl:ObjectProperty ;
                 owl:inverseOf :ownsChampion ;
                 rdfs:domain :Champion ;
                 rdfs:range vgo:Player ;
                 rdfs:comment "Indicates that a champion in MOBA games is owned or possessed by a player. This property represents the reverse relationship of ownsChampion."@en ;
                 rdfs:label "isOwnedByPlayer"@en .


###  http://www.semanticweb.org/MobaOntology#isPlatformOf
:isPlatformOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Platform ;
              rdfs:range vgo:Game .


###  http://www.semanticweb.org/MobaOntology#isPlayableOf
:isPlayableOf rdf:type owl:ObjectProperty ;
              rdfs:comment "Indicates an entity that is a playable within a game. This property links a playable character to the game in which it can be played."@en ;
              rdfs:label "isPlayableOf"@en .


###  http://www.semanticweb.org/MobaOntology#isPlayedByTeam
:isPlayedByTeam rdf:type owl:ObjectProperty ;
                owl:propertyChainAxiom ( :hasMatch
                                         :includesTeam
                                       ) .


###  http://www.semanticweb.org/MobaOntology#isRoleOf
:isRoleOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Role ;
          rdfs:range :Champion .


###  http://www.semanticweb.org/MobaOntology#ownsChampion
:ownsChampion rdf:type owl:ObjectProperty ;
              rdfs:domain vgo:Player ;
              rdfs:range :Champion ;
              rdfs:comment "Indicates ownership or possession of a playable champion by a player in MOBA games. A champion referred to by this property is actively played by the owner, distinguishing it from non-playable characters."@en ;
              rdfs:label "ownsChampion"@en .


###  http://www.semanticweb.org/MobaOntology#suitableItem
:suitableItem rdf:type owl:ObjectProperty ;
              owl:propertyChainAxiom ( :hasBuild
                                       :hasItemSet
                                     ) .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/net/videogameontology#username
vgo:username rdf:type owl:DatatypeProperty ;
             owl:equivalentProperty :hasNickname ;
             rdfs:comment "The username connects player to a text string which describes the username a player has."@en ;
             rdfs:label "username"@en .


###  http://www.semanticweb.org/MobaOntology#abilityName
:abilityName rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/MobaOntology#componentQuantity
:componentQuantity rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/MobaOntology#hasArmor
:hasArmor rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :hasBaseStat ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Champion ;
          rdfs:range xsd:integer ;
          rdfs:comment "Specifies the base armor of the champion."@en ;
          rdfs:label "hasArmor"@en .


###  http://www.semanticweb.org/MobaOntology#hasAttackDamage
:hasAttackDamage rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :hasBaseStat ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Champion ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "Specifies the base attack damage of the champion."@en ;
                 rdfs:label "hasAttackDamage"@en .


###  http://www.semanticweb.org/MobaOntology#hasBaseStat
:hasBaseStat rdf:type owl:DatatypeProperty ;
             rdfs:comment "Represents the basic properties and attributes of a champion in a MOBA game."@en ;
             rdfs:label "hasBaseStat"@en .


###  http://www.semanticweb.org/MobaOntology#hasFoundationDate
:hasFoundationDate rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:comment "Specifies the date when a software development company (SoftwareHouse) was founded. This property records the foundation date of the company."@en ;
                   rdfs:label "hasFoundationDate"@en .


###  http://www.semanticweb.org/MobaOntology#hasHealth
:hasHealth rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :hasBaseStat ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Champion ;
           rdfs:range xsd:integer ;
           rdfs:comment "Specifies the base health points of the champion."@en ;
           rdfs:label "hasHealth"@en .


###  http://www.semanticweb.org/MobaOntology#hasIndividualName
:hasIndividualName rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:comment "Specifies the real name of an individual developer. This property records the personal name of the developer."@en ;
                   rdfs:label "hasIndividualName"@en .


###  http://www.semanticweb.org/MobaOntology#hasMagicResist
:hasMagicResist rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :hasBaseStat ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :Champion ;
                rdfs:range xsd:integer ;
                rdfs:comment "Specifies the base magic resist of the champion."@en ;
                rdfs:label "hasMagicResist"@en .


###  http://www.semanticweb.org/MobaOntology#hasMana
:hasMana rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :hasBaseStat ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain :Champion ;
         rdfs:range xsd:integer ;
         rdfs:comment "Specifies the base mana pool of the champion. Mana is a resource used to cast spells and abilities."@en ;
         rdfs:label "hasMana"@en .


###  http://www.semanticweb.org/MobaOntology#hasMovementSpeed
:hasMovementSpeed rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasBaseStat ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Champion ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "Specifies the base movement speed of the champion."@en ;
                  rdfs:label "hasMovementSpeed"@en .


###  http://www.semanticweb.org/MobaOntology#hasName
:hasName rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Champion ;
         rdfs:range xsd:string ;
         rdfs:comment "Specifies the name of the champion."@en ;
         rdfs:label "hasName"@en .


###  http://www.semanticweb.org/MobaOntology#hasNickname
:hasNickname rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain vgo:Player ;
             rdfs:range xsd:string ;
             rdfs:comment "Specifies the nickname or in-game alias of a player in MOBA games. This property is used to identify players by their chosen game names."@en ;
             rdfs:label "hasNickname"@en .


###  http://www.semanticweb.org/MobaOntology#hasRange
:hasRange rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :hasBaseStat ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Champion ;
          rdfs:range xsd:string ;
          rdfs:comment "Specifies the attack range of the champion, indicating whether the champion is melee or ranged."@en ;
          rdfs:label "hasRange"@en .


###  http://www.semanticweb.org/MobaOntology#isOfficialCompetition
:isOfficialCompetition rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:boolean .


###  http://www.semanticweb.org/MobaOntology#itemCost
:itemCost rdf:type owl:DatatypeProperty ;
          rdfs:domain vgo:Item ;
          rdfs:range xsd:integer .


###  http://www.semanticweb.org/MobaOntology#itemEffect
:itemEffect rdf:type owl:DatatypeProperty ;
            rdfs:domain vgo:Item ;
            rdfs:range xsd:string .


###  http://www.semanticweb.org/MobaOntology#itemName
:itemName rdf:type owl:DatatypeProperty ;
          rdfs:domain vgo:Item ;
          rdfs:range xsd:string .


###  http://www.semanticweb.org/MobaOntology#priceToPlay
:priceToPlay rdf:type owl:DatatypeProperty ;
             rdfs:domain :MonetizationModel ;
             rdfs:range xsd:integer ;
             rdfs:comment "Represents the price required to play a MOBA videogame."@en ;
             rdfs:label "priceToPlay"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/NET/c4dm/event.owl#Event
event:Event rdf:type owl:Class ;
            rdfs:comment """An arbitrary classification of a space/time region, by a
cognitive agent. An event may have actively participating agents,
passive factors, products, and a location in space/time."""@en ;
            rdfs:label "Event"@en .


###  http://purl.org/dc/dcmitype/Collection
dctype:Collection rdf:type owl:Class ;
                  rdfs:comment "A collection is described as a group; its parts may also be separately described."@en ;
                  rdfs:label "Collection"@en .


###  http://purl.org/net/videogameontology#Character
vgo:Character rdf:type owl:Class ;
              rdfs:comment "A vgo:Character is any actor that can exists in a game. A character can be a human-like creature as seen traditionally in video games. However, a character could also be a car, a paddle in game of Pong or spaceship of a space shooter game. This is often contextually related to the gameplay of a certain game. A character can be either controller by a player or by a computer."@en ;
              rdfs:label "Character"@en .


###  http://purl.org/net/videogameontology#GainEvent
vgo:GainEvent rdf:type owl:Class ;
              rdfs:subClassOf vgo:GameEvent ;
              rdfs:comment "The vgo:GainEvent describes an event that is related to character/player gaining something in a game. This is a subclass of vgo:GameEvent as gaining something is related to a specific game. For example, a player can gain a new character, achievement or item."@en ;
              rdfs:label "GainEvent"@en .


###  http://purl.org/net/videogameontology#Game
vgo:Game rdf:type owl:Class ;
         rdfs:comment "The vgo:Game class describes a Game Product that can be played by a player."@en ;
         rdfs:label "Game"@en .


###  http://purl.org/net/videogameontology#GameEvent
vgo:GameEvent rdf:type owl:Class ;
              rdfs:subClassOf vgo:InstantaneousEvent ;
              rdfs:comment "The vgo:GameEvent describes an event that takes place in a game without straight player interaction. GameEvents are often very specific for each game. Examples of vgo:GameEvent could be an enemy dying, connecting to a multiplayer server, loading a new level or playing an animation."@en ;
              rdfs:label "GameEvent"@en .


###  http://purl.org/net/videogameontology#GameProduct
vgo:GameProduct rdf:type owl:Class ;
                rdfs:comment """A vgo:GameProduct is anything that is for sale inside a game. These can be either normal game items purchased with in-game currency or with real world money. An example of vgo:GameProduct could be a consumable health potion bought with real money, a better weapon or some visual improvement (e.g. Hats in Steam). Basically a Game Product can be anything, a character, an item or an achievement.

GameProduct is a subclass of Good Relations: ProductOrService & schema:Product. Since vgo:GameProduct is a type of buyable product, it reuses the properties available in the schema and Good Relations, such as currency price, validity of the offer and so on."""@en ;
                rdfs:label "GameProduct"@en .


###  http://purl.org/net/videogameontology#InAppPurchaseEvent
vgo:InAppPurchaseEvent rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty vgo:purchasesGameOffering ;
                                             owl:someValuesFrom vgo:GameProduct
                                           ] ;
                       rdfs:subClassOf vgo:InstantaneousEvent ;
                       rdfs:comment "The vgo:InAppPurchaseEvent describes an event that is related to making a purchase with real money inside a game. This is a subclass of InstantaneousEvent because it happens at certain moment in time. An example of vgo:InAppPurchaseEvent would be unlocking secret levels with real money or purchasing better equipment with real money."@en ;
                       rdfs:label "InAppPurchaseEvent"@en .


###  http://purl.org/net/videogameontology#InstantaneousEvent
vgo:InstantaneousEvent rdf:type owl:Class ;
                       rdfs:comment "The vgo:InstantaneousEvent class describes an event that happens during the gameplay at a certain moment in time. This can be a player gaining an achievement, killing an enemy or making an In-app purchase."@en ;
                       rdfs:label "InstantaneousEvent"@en .


###  http://purl.org/net/videogameontology#Item
vgo:Item rdf:type owl:Class ;
         rdfs:comment "A vgo:Item portrays any item that exists in a game. The item can either be just visual part of the game or a concrete usable item. As an example an item could be a drinkable potion, a magical sword or just a flower pot."@en ;
         rdfs:label "Item"@en .


###  http://purl.org/net/videogameontology#LoseEvent
vgo:LoseEvent rdf:type owl:Class ;
              rdfs:subClassOf vgo:GameEvent ;
              rdfs:comment "vgo:LoseEvent describes an event that is related to character/player losing something in a game. This is a subclass of GameEvent as gaining something is related to a specific game. For example, a player can lose a character due to trade with another player. Another example would be a character losing item due to consuming it."@en ;
              rdfs:label "LoseEvent"@en .


###  http://purl.org/net/videogameontology#MenuEvent
vgo:MenuEvent rdf:type owl:Class ;
              rdfs:subClassOf vgo:InstantaneousEvent ;
              rdfs:comment "The vgo:MenuEvent describes an event that is related to interacting with the ingame menu. An example of Menu event is muting sounds, changing graphic settings, changing gameplay difficulty or remapping game controls."@en ;
              rdfs:label "MenuEvent"@en .


###  http://purl.org/net/videogameontology#Player
